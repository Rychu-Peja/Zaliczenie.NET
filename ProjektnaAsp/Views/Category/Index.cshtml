@model List<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
	<thead>
	<tr>
		<th>
			Category name
		</th>
		<th>
			Display Order
		</th>
	</tr>
	</thead>
	<tbody>
		@foreach (var obj in Model)
		{
			<tr>
				<td>Name</td>
				<td>
				DisplayOrder
				</td>
			</tr>
		}
	</tbody>
</table>